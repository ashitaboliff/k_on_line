generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model booking {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now())
  booking_date DateTime
  booking_time Int
  regist_name  String
  name         String
  password     String
}
